[color]
	branch = auto
	diff = auto
	status = auto
	ui = true
[core]
	autocrlf = input
	editor = zed --wait
	excludesfile = ~/.gitignore
	quotepath = false
[diff]
	tool = zed
[difftool "vim"]
	cmd = vim -d $LOCAL $REMOTE
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
[difftool "zed"]
	cmd = zed --wait --diff $LOCAL $REMOTE
[gc]
	auto = 3000
	autopacklimit = 20
[merge]
	ff = true
	tool = zed
[mergetool "vim"]
	cmd = vim -d $LOCAL $MERGED $REMOTE
	keepbackup = false
[mergetool "vscode"]
	cmd = code --wait $MERGED
[mergetool "zed"]
	cmd = zed --wait $MERGED
[pull]
	rebase = false
[transfer]
	fsckObjects = false
	unpackLimit = 10
[user]
	email = 84360+japboy@users.noreply.github.com
	name = Yu I.
[init]
	defaultBranch = main
[url "https://<PAT>:x-oauth-basic@github.com/"]
	insteadOf = https://github.com/
